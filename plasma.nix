{lib, ...}: {
  programs.plasma = {
    enable = true;
    workspace = {
      # plasma-apply-lookandfeel --list
      lookAndFeel = "org.kde.breezedark.desktop";
      theme = "breeze-dark";
    };
    fonts = {
      general = {
        family = "Noto Sans";
        pointSize = 10;
      };
      small = {
        family = "Noto Sans";
        pointSize = 8;
      };
      fixedWidth = {
        family = "FiraCode Nerd Font";
        pointSize = 10;
      };
    };
    panels = [
      {
        location = "top";
        height = 24;
        widgets = [
          "org.kde.plasma.kickoff"
          {
            pager = {
              general.showWindowOutlines = true;
            };
          }
          {
            iconTasks = {
              launchers = [];
              behavior.showTasks.onlyInCurrentScreen = true;
            };
          }
          "org.kde.plasma.marginsseparator"
          {
            systemMonitor = {
              displayStyle = "org.kde.ksysguard.linechart";
              title = "Total CPU Use";
              sensors = [{
                name = "cpu/all/usage";
                color = "61,174,233";
                label = "cpu/all/usage";
              }];
            };
          }
          {
            systemMonitor = {
              displayStyle = "org.kde.ksysguard.linechart";
              title = "Total memory Use";
              sensors = [{
                name = "memory/physical/used";
                color = "61,174,233";
                label = "memory physical used";
              }];
            };
          }
          {
            systemMonitor = {
              displayStyle = "org.kde.ksysguard.horizontalbars";
              title = "Disk Usage";
              sensors = [
                {
                  name = "disk/.*/usedPercent";
                  color = "61,174,233";
                  label = "disk/.*/usedPercent";
                }
                {
                  name = "disk/all/usedPercent";
                  color = "233,120,61";
                  label = "disk/all/usedPercent";
                }
                # {
                #   name = "disk/c092a5fc-6860-4784-a89b-0b6bd62a09a9/usedPercent";
                #   color = "61,174,233";
                # }
                # {
                #   name = "disk/fd6bffd7-418c-440f-9c9d-0d605903a603/usedPercent";
                #   color = "233,120,61";
                # }
              ];
            };
          }
          "org.kde.plasma.systemtray"
          {
            digitalClock = {
              date = {
                enable = true;
                format = "isoDate";
                position = "besideTime";
              };
              time = {
                showSeconds = "always";
                format = "24h";
              };
              calendar = {
                firstDayOfWeek = "sunday";
              };
            };
          }
        ];
      }
    ];
    kscreenlocker.timeout = 5;
    kwin = {
      borderlessMaximizedWindows = true;
      effects.desktopSwitching.animation = "slide";
      effects.shakeCursor.enable = true;
      # TODO deberia ver que onda con los colores de esto
      effects.translucency.enable = true;
      effects.dimInactive.enable = true;
    };
    powerdevil = {
      AC = {
        autoSuspend.action = "nothing";
        inhibitLidActionWhenExternalMonitorConnected = true;
        dimDisplay.enable = true;
        dimDisplay.idleTimeout = 300;
        powerButtonAction = "showLogoutScreen";
        whenLaptopLidClosed = "sleep";
